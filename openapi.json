    "/api/instagram/search": {
      "post": {
        "operationId": "searchInstagram",
        "summary": "Search Instagram creators",
        "description": "Search for Instagram creators using advanced filters. Supports filtering by displayName, follower count, engagement rate, country, language, niches, and more.",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["filters"],
                "properties": {
                  "filters": {
                    "type": "array",
                    "description": "Array of filter objects. Each filter has: filterName, op ('in', '>', '=', '<'), value, isFuzzySearch (optional)",
                    "items": {
                      "type": "object",
                      "properties": {
                        "filterName": {"type": "string", "description": "Field to filter (displayName, totalFollowers, country, mainLanguage, niches, avgEngagementRate, isVerified)"},
                        "op": {"type": "string", "enum": ["in", ">", "=", "<"]},
                        "value": {"description": "Filter value"},
                        "isFuzzySearch": {"type": "boolean"}
                      }
                    }
                  },
                  "pageSize": {"type": "integer", "default": 20, "minimum": 1, "maximum": 100},
                  "offset": {"type": "integer", "default": 0},
                  "sortBy": {"type": "string"},
                  "desc": {"type": "boolean", "default": true}
                }
              }
            }
          }
        },
        "responses": {"200": {"description": "Search results"}}
      }
    },
